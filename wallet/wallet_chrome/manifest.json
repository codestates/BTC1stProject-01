{
  "name": "놀면뭐하모니 지갑",
  "description": "하모니(Harmony ONE) 블록체인 지갑(Wallet) 입니다",
  "version": "0.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    },
    "icons": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    }
  },
  "options_page": "options.html",
  "manifest_version": 3
}